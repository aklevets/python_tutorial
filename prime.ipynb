      "source": [
        "def is_prime (number):\n",
        "  i = 2\n",
        "  res = True\n",
        "\n",
        "  while i <= round(number / 2):\n",
        "    if(number % i == 0): res = False\n",
        "    i = i + 1\n",
        "\n",
        "  return res"
      ]
    },
